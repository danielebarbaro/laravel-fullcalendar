{
    "name": "maddhatter/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn Tunney",
            "email": "shawn.tunney@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "illuminate/support": "8.*",
        "illuminate/contracts": "8.*",
        "laravel/helpers": "1.*"
    },
    "autoload": {
        "psr-4": {
            "MaddHatter\\LaravelFullcalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaddHatter\\LaravelFullcalendar\\CalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "MaddHatter\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
